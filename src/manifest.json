{
    "name": "Name instead of UserId for SAP GitHub",
    "description": "Display full name instead of UserId for SAP GitHub Enterprise instance",
    "version": "0.5.0",
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "index.bundle.js"
        ],
        "matches": [
            "https://github.wdf.sap.corp/*",
            "https://github.tools.sap/*"
        ],
        "exclude_matches": [
            "https://github.wdf.sap.corp/pages*",
            "https://pages.github.tools.sap/*"
        ]
    }],
    "permissions": [
    	"storage"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
